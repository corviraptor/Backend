{
    "type":"origins:action_over_time",
    "hidden": true,
    "entity_action": {
        "type": "origins:execute_command",
        "command": "scoreboard players operation @s ComboBladeDrawTime -= one Constants"
    },
    "condition": {
        "type":"origins:scoreboard",
        "objective": "ComboBladeDrawTime",
        "compare_to": 0,
        "comparison": "!="
    },
    "interval": 2,
    "falling_action": {
        "type":"origins:if_else",
        "condition": {
            "type": "origins:predicate",
            "predicate": "dredgecombat:comboblade/holding_axe"
        },
        "if_action": {
            "type": "origins:and", 
            "actions": [
            {
                "type": "origins:execute_command",
                "command": "function dredgecombat:comboblade/end_draw"
            },
            {
                "type":"origins:area_of_effect",
                "bientity_action": {
                    "type": "origins:damage",
                    "amount": 20,
                    "damage_type": "minecraft:player_attack"
                },
                "radius": 4,
                "shape": "cube"
            }]
        },
        "else_action": {
            "type": "origins:execute_command",
            "command": "tellraw @s { \"text\": \"Predicate Failed!!\", \"color\": \"#FF0000\" }"
        }
        
    }
    
}