{
    
    "type":"origins:action_over_time",

    "hidden": true,

    "entity_action": {
        "type": "origins:execute_command",
        "command": "scoreboard players operation @s dredgecombat:MoveTime -= #TIMER_INTERVAL dredgecombat:MoveTime"
    },
    
    "condition": {
        "type":"origins:scoreboard",
        "objective": "dredgecombat:MoveTime",
        "compare_to": 0,
        "comparison": "<="
    },

    "interval": 2,

    "falling_action": {
        "type": "origins:if_else",
        "condition": {
            "type": "origins:resource",
            "resource": "dredgecombat:comboblade/draw/timer",
            "compare_to": 0,
            "comparison": "=="
        },
        "if_action": {
            "type":"origins:emit_game_event",
            "event": "minecraft:block_open"
        },
        "else_action": {
            "type":"origins:emit_game_event",
            "event": "minecraft:dispense_fail"
        }
    }
}