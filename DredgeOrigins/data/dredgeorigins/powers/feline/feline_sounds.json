{
    "type": "origins:multiple",
    "hidden": true,
    "hurt_death": {
        "type": "origins:self_action_when_hit",
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:health",
                "comparison": ">",
                "compare_to": 0
            },
            "if_action": {
                "type": "origins:play_sound",
                "sound": "minecraft:entity.cat.hurt"
            },
            "else_action": {
                "type": "origins:play_sound",
                "sound": "minecraft:entity.cat.death"
            }

        },
        "cooldown": 1
    },
    "idle": {
        "type": "origins:action_over_time",
        "interval": 100,
        "entity_action": {
            "type": "origins:chance",
            "action": {
                "type": "origins:if_else_list",
                "actions": [
                    {
                        "condition": {
                            "type": "origins:health",
                            "comparison": ">",
                            "compare_to": 4
                        },
                        "action": {
                            "type": "origins:play_sound",
                            "sound": "minecraft:entity.cat.ambient"
                        }
                    },
                    {
                        "condition": {
                            "type": "origins:health",
                            "comparison": "<=",
                            "compare_to": 4
                        },
                        "action": {
                            "type": "origins:play_sound",
                            "sound": "minecraft:entity.cat.ambient",
                            "pitch": 0.8
                        }
                    }
                ]
            },
            "chance": 0.05
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:sneaking",
                    "inverted": true
                },
                {
                    "type": "origins:gamemode",
                    "gamemode": "spectator",
                    "inverted": true
                },
                {
                    "type": "origins:status_effect",
                    "effect": "minecraft:invisibility",
                    "inverted": true
                },
                {
                    "type": "origins:origin",
                    "origin": "origins-classes:rogue",
                    "layer": "origins-classes:class",
                    "inverted": true
                }
            ]
        }
    },
    "meow": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:play_sound",
            "sound": "minecraft:entity.cat.ambient"
        }
    },
    "purr": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:play_sound",
            "sound": "minecraft:entity.cat.purr"
        },
        "key": {
            "key": "key.origins.secondary_active"
        }
    },
    "beg": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:play_sound",
            "sound": "minecraft:entity.cat.beg_for_food"
        },
        "key": {
            "key": "key.origins.senary_active"
        }
    },
    "hiss": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:play_sound",
            "sound": "minecraft:entity.cat.hiss"
        },
        "key": {
            "key": "key.origins.septenary_active"
        }
    },
    "stray": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:play_sound",
            "sound": "minecraft:entity.cat.stray_ambient"
        },
        "key": {
            "key": "key.origins.octonary_active"
        }
    }
}
